#include <iostream>
#include <vector>
#include <iterator>
using namespace std;
int main()
{
    vector<int> ar = { 11, 23, 43, 48, 5, 12, 29 };
    ar.push_back(11);
    ar.push_back(23);
    ar.push_back(43);
    ar.push_back(48);
    ar.push_back(5);
    ar.push_back(12);
    ar.push_back(29);
    vector<int>::iterator ptr1,ptr2;
    
    ptr1 = ar.begin();
    cout << "First element is : " << *ptr1 << "\n";
    // [cout << ptr1 << "\n";] will give error
    vector<int>::iterator ptr2;
    
    ptr2 = ar.end();
    //[cout << "Last element is : " << *ptr2 << "\n";]  wrong statement
    //[cout << ptr1 << "\n";] is also wrong statement
    
    //Last or any element using prev and next
    #Last element
    vector<int>::iterator ptrLast;
    ptrLast = prev(ptr2, 1);
    cout << "Last element is : " << *ptrLast << "\n";
    
    #Last second element
    vector<int>::iterator ptrLast2 = prev(ptrr2,2);

    #Second element of vector
    vector<int>::iterator ptr2nd;
    ptr2nd = next(ptr1, 1);
    cout << "Second element is : " << *ptr2nd << "\n";
    
    #Third element
    vector<int>::iterator ptrThird = next(ptr1,2);
}


Output:
First element is : 11
Last element is : 29
Second element is : 23

#include <bits/stdc++.h>

using namespace std;

int main()
{
    string sn = "1235";
    
    stringstream ss(sn);
    int x = 0;
    ss >> x;
    cout << x << endl;
    cout << x * 10 << endl;

    long long int dx = 0;
    // ss >> dx; --> Wrong
    /*
    stringstream ss(sn);
    ss >> dx;
    This would also be wrong.ss can not be initialized again
    */
    stringstream ss1(sn); 
    ss1 >> dx;
    cout << dx << endl;
    cout << dx / 5 << endl;
}

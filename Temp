Also,
#include <bits/stdc++.h>
#include <iostream>
using std::cout;
using std::cin;
int main()
{
    cout << "Hello world!(1)"<< "\n";
    cout << "Hello world!(2)\n";
    cout << "Hello world!(3)\n";
    cout<<8<<" ";
    cout<<9<<"\n";
    cout << "Hello world!(4)\n";
    cout<<56<<" "<<6<<"\n";
    return 0;
}

#include<iostream>
#include<vector>
using namespace std;
int main()
{
  
  int n;
  cin >> n;
  vector<int> vec[n];
  int k,x;
  for(int i = 0;i<n;i++)
  {
  	cin >> k;
  	for(int j = 1;j<=k;j++)
  	{
	  	cin >> x;
	  	vec[i].push_back(x);
	}
  }
  int sz;
  for(int i = 0;i<n;i++)
  {
  	cout << i << " no. vectore = "<<vec[i][0] ;
  	sz = vec[i].size();
  	for(int j = 1;j<sz;j++)
  	{
  	  cout << " ," <<vec[i][j];	
	}
	cout<<"\n";
  }
  return 0;
}

INPUT :
2 3 1 5 4 5 1 2 8 9 3

OUTPUT :
2 3 1 5 4 5 1 2 8 9 3
0 no. vectore = 1 ,5 ,4
1 no. vectore = 1 ,2 ,8 ,9 ,3

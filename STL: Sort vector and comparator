#include <iostream>
#include <vector>
#include <algorithm>
using namespace std;
bool sort_func(int x, int y)
{
    return x > y;
}
int main()
{
    vector< int > v = { 23,67,56,89,67,34 };
    sort(v.begin(),v.end());
    for (int i = 0; i < v.size(); i++)
        cout << v[i] << "\n";

    cout << "\n";

    vector< int > v1 = { 23,67,56,89,67,34 };
    // Sort in descending order using comparator
    sort(v1.begin(), v1.end(), sort_func);
    for (int i = 0; i < v1.size(); i++)
        cout << v1[i] << "\n";

}

#include <vector>
#include <iostream>
#include <bitset>
#include <math.h>
//#include <bits/stdc++.h>
using namespace std;
void multiply_func(vector<int>& rslt, int n)
{
	int sz = rslt.size();
	int car = 0;
	int prd = 0;
	for (int i = 0; i < sz; i++)
	{
		prd = rslt[i] * n + car;
		car = prd / 10;
		prd = prd % 10;
		rslt[i] = prd;
	}
	while (car != 0)
	{
		rslt.push_back(car % 10);
		car = car / 10;
	}
}


void fact(int n)
{
	if (n < 1)
		cout << 1 << endl;

	vector<int> rslt;
	rslt.push_back(1);
	for (int i = 1; i <= n; i++)
	{
		multiply_func(rslt, i);
		
	}
	reverse(rslt.begin(), rslt.end());
	int sz = rslt.size();
	for (int i = 0; i < sz; i++)
	{
		cout << rslt[i];
	}
	cout << endl;

}

int main()
{

	int t;
	cin >> t;
	while (t-- > 0)
	{
		int n;
		cin >> n;
		fact(n);
	}
}

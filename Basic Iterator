#include <iostream>
#include <vector>
#include <iterator>
using namespace std;
int main()
{
    vector<int> ar = { 11, 23, 43, 48, 5, 12, 29 };
    vector<int>::iterator ptr1;
    ptr1 = ar.begin();

    


    cout << "First element is : " << *ptr1 << "\n";

    vector<int>::iterator ptr2;
    ptr2 = ar.end();
    //cout << "Last element is : " << *ptr2 << "\n"; --> Wrong statement
    
    //Last or any element using prev and next
    //Last element
    vector<int>::iterator ptrF;
    ptrF = prev(ptr2, 1);
    cout << "Last element is : " << *ptrF << "\n";
    //prev(ptrr2,2) --> Last second element

    //Second element of vector
    vector<int>::iterator ptr2nd;
    ptr2nd = next(ptr1, 1);
    cout << "Second element is : " << *ptr2nd << "\n";
    //next(ptr1,2) --> third element
}


Output:
First element is : 11
Last element is : 29
Second element is : 23

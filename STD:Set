#include <iostream>
#include <set> 
#include <iterator> 
using namespace std;
int main()
{

    set<int> st;
    //insert//
    st.insert(90);
    st.insert(67);
    st.insert(12);
    st.insert(23);
    st.insert(45);
    st.insert(39);
    st.insert(12);
    //Traverse method : 01//
    cout << "Traverse Method : 01\nSet = ";
    for (auto elem : st)
    {
        cout << " " <<elem;
    }
    cout << endl;
    cout << "\n" << endl;

    //Traverse method : 02//
    cout << "Traverse Method : 02\nSet = ";
    set<int>::iterator  b , e;
    b = st.begin();
    e = st.end();
    set<int>::iterator it;
    for (it = b; it != e; it++)
    {
        cout << " " <<*it ;
    }
    cout << endl;
    cout << "\n" << endl;

    //Erase
    st.erase(40);
    st.erase(45);
    cout << "Set : ";
    for (it = b; it != e; it++)
    {
        cout << " " << *it;
    }
    cout << endl;
    cout << "\n" << endl;

    //Find
    set<int>::iterator f;
    f = st.find(50); // If found then f = iterator at that position or  f = st.end()
    //cout << (f - b) << endl; -->will cause error
    if (f != e)
        cout << *f << endl;
    else
        cout << "Not Found" << endl;
    f = st.find(39);
    if (f != e)
        cout << *f << endl;
    else
        cout << "Not Found" << endl;
    cout << "\n" << endl;

    //Lower bound
    set<int>::iterator lb = lower_bound(b,e,45);
    //If lb != e
    if(lb!=e)
        cout << *lb << endl;
    else
        cout << "Not here" << endl;
    // cout<<lb-b<<endl; --> Wrong again
    // Set doesn't support iterator - iterator
}
